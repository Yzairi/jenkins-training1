pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                git url: 'https://github.com/Yzairi/jenkins-training1.git'
            }
        }
        stage('Build') {
            steps {
                withMaven(
                    maven: 'MAVEN_3_9_9',
                    jdk: 'JDK-21'
                ) {
                    sh 'mvn package'
                }
            }
        }
    }
}
